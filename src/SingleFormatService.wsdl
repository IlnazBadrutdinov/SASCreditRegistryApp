							<xsd:documentation>Компания-работодатель: сокращённое название</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="empFirmName" type="xsd:string" nillable="true" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Компания-работодатель: фирменное наименование</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="empEngName" type="xsd:string" nillable="true" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Компания-работодатель: наименование на иностранном языке</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
